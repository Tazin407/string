#include<stdio.h>
#include<string.h>

int main(){
      char arr1[21];
      char arr2[21];
      scanf("%s %s", arr1, arr2);
      for (int i = 0; i <=21; i++)
      {
         if (arr1[i]==arr2[i])
         {
            continue;
         }
         
         else if (arr1[i]<arr2[i])
          {
            printf("%s", arr1);
            break;
          }
          else{
            printf("%s", arr2);
            break;
          }
          
      }
      
      
      
    return 0;
}
