#include<stdio.h>
#include<string.h>

int main(){
      char a[100], b[100];
      scanf("%s %s", a,b);
      int v= strlen(a);
      //logical
     for (int i = 0; i <=strlen(b); i++)
     {
         a[v]=b[i];
         v++;
     }
      //function
      //strcat(a,b);
      printf("%s", a);

    return 0;
}
